            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-league"</span><span class="nt">&gt;</span><span>League Gothic webfont</span><span class="nt">&lt;/h2&gt;</span>
            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-typoslab"</span><span class="nt">&gt;</span><span>Typo Slab Serif</span><span class="nt">&lt;/h2&gt;</span>
            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-modernsans"</span><span class="nt">&gt;</span><span>Modern Sans Regular</span><span class="nt">&lt;/h2&gt;</span>
            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-allura"</span><span class="nt">&gt;</span><span>Allura Regular</span><span class="nt">&lt;/h2&gt;</span>
            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-alfaslab"</span><span class="nt">&gt;</span><span>Alfa Slab One Regular</span><span class="nt">&lt;/h2&gt;</span>
            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-amaranth"</span><span class="nt">&gt;</span><span>Amaranth Font Regular</span><span class="nt">&lt;/h2&gt;</span>
            <span class="nt">&lt;h2</span> <span class="na"> class=</span><span class="s">"f-amaranthbold"</span><span class="nt">&gt;</span><span>Amaranth Font Bold</span><span class="nt">&lt;/h2&gt;</span>
